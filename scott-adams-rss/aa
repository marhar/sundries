<script type='text/javascript'>
  var browserWidth = $(window).width();
  var browserHeight = $(window).height();

  var adSlotPath = '/19196947/dilbert.com/blog/scottadams';

  /* ************************** */
   /*          Ad Config         */
   /* ************************** */
   var adSlotConfig = {};
   adSlotConfig['leaderboard'] = {
       name: 'leaderboard',
       targeting: {'pos': 'top'},
       breakpoints: [
           {
               minWidth: 970,
               minHeight: 850,
               sizes: [[970, 250], [970, 90], [728, 90]]
           },
           {
               minWidth: 970,
               minHeight: 0,
               sizes: [[970, 90], [728, 90]]
           },
           {
               minWidth: 728,
               minHeight: 0,
               sizes: [[728, 90], [320, 50]]
           },
           {
               minWidth: 0,
               minHeight: 0,
               sizes: [[320, 50]]
           }
       ]
   };

   adSlotConfig['rectangle_left'] = {
       name: 'rectangle_left',
       targeting: {'pos': 'left'},
       breakpoints: [
           {
               minWidth: 0,
               minHeight: 0,
               sizes: [[300, 250]]
           }
       ]
   };

   adSlotConfig['rectangle_right'] = {
       name: 'rectangle_right',
       targeting: {'pos': 'right'},
       breakpoints: [
           {
               minWidth: 1400,
               minHeight: 0,
               sizes: [[300, 250]]
           },
           {
               minWidth: 0,
               minHeight: 0,
               sizes: [[]]
           }
       ]
   };

   adSlotConfig['rectangle_post'] = {
       name: 'rectangle_post',
       targeting: {'pos': 'post'},
       breakpoints: [
           {
               minWidth: 0,
               minHeight: 0,
               sizes: [[300, 250]]
           }
       ]
   };


   adSlotConfig['tower'] = {
       name: 'tower',
       targeting: {'pos': 'anchor'},
       breakpoints: [
           {
               minWidth: 550,
               minHeight: 0,
               sizes: [[160, 600], [300, 600]]
           },
           {
               minWidth: 0,
               minHeight: 0,
               sizes: [[]]
           }
       ]
   };



   /* ********************************** */
   /*      Helper Functions for Ads      */
   /* ********************************** */

   var findBreakpoint = function(breakpointsArray) {
       var maxBpt, curBpt;
       for (var i = 0; i < breakpointsArray.length; i++) {
           curBpt = breakpointsArray[i];
           if (browserWidth >= curBpt.minWidth && browserHeight >= curBpt.minHeight){
               if(!maxBpt || (curBpt.minWidth >= maxBpt.minWidth && curBpt.minHeight >= maxBpt.minHeight)){
                   maxBpt = curBpt;
               }
           }
       }
       return maxBpt
   };

   function findAds(selector) {
        var ads = $(selector);

        // Filter out any ads that are hidden
        var visibleAds = ads.filter(":visible");

        return visibleAds;
    }

    function defineAds(adDivs) {
        var slots = [];
        var i, slotType, slot;
        for (i = 0; i < adDivs.length; i++) {
            // ID given to ad slot, usually ad type + index
            slotName = adDivs[i].id;

            // Slot type, stored in data-ad-type
            slotType = $(adDivs[i]).data('ad-type');

            // Define slot
            slot = googletag.defineSlot(adSlotPath, adSlotConfig[slotType].breakpoint.sizes, slotName).addService(googletag.pubads());

            // Set slot level targeting
            for (key in adSlotConfig[slotType].targeting) {
                slot.setTargeting(key, adSlotConfig[slotType].targeting[key]);
            }

            slots.push(slot);

        }
        return slots;
    }

    function displayAds(adDivs) {
        var adDivsLength = adDivs.length;
        var slotName;

        if(adDivsLength == 0) {
            googletag.display();
        } else {
            for (var i = adDivsLength-1; i>=0; i--) {
                slotName = adDivs[i].id;
                googletag.display(slotName);
            }
        }

    }

    function amzn_buildSlotsArray (slots) {
        var slotsArray = [];
        var i, slot, slotType;
        for(i = 0; i < slots.length; i++){
            slot = slots[i];
            slotType = $(slot).data('ad-type');
            slotsArray.push({
                slotID: slot.id,
                sizes: adSlotConfig[slotType].breakpoint.sizes
            });
        }
        return slotsArray;

    }

    // Set all breakpoints
    adSlotConfig['leaderboard'].breakpoint = findBreakpoint(adSlotConfig['leaderboard'].breakpoints);
    adSlotConfig['rectangle_left'].breakpoint = findBreakpoint(adSlotConfig['rectangle_left'].breakpoints);
    adSlotConfig['rectangle_right'].breakpoint = findBreakpoint(adSlotConfig['rectangle_right'].breakpoints);
    adSlotConfig['rectangle_post'].breakpoint = findBreakpoint(adSlotConfig['rectangle_post'].breakpoints);
    adSlotConfig['tower'].breakpoint = findBreakpoint(adSlotConfig['tower'].breakpoints);


    /* *************************** */
    /*      Initial Page Load      */
    /* *************************** */
    googletag.cmd.push(function() {
       $(document).ready(function(){

           var curAds = findAds('.js-ad');
           var curSlots = defineAds(curAds);

           // Fetch Amazon bids for slots on page
           apstag.fetchBids(
               {
                   slots: amzn_buildSlotsArray(curAds),
                   timeout: 1000
               },
               function(bids) {
                   googletag.cmd.push(function(){
                       // Set Amazon bid key-values pairs
                       try {
                           apstag.setDisplayBids();
                       } catch(e) {
                           //do nothing
                       }

                       // Enable services on googletag
                       googletag.enableServices();

                       // Display ads
                       displayAds(curAds);
                   });
               }
           );
       });
   });

</script>

